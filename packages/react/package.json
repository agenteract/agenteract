{
  "name": "@agenteract/react",
  "version": "0.0.1",
  "description": "React and React Native bridge utilities for Agenteract",
  "main": "./dist/react/src/index.js",
  "types": "./dist/react/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest --config jest.config.cjs"
  },
  "exports": {
    ".": {
      "import": "./dist/react/src/index.js",
      "require": "./dist/react/src/index.js",
      "types": "./dist/react/src/index.d.ts"
    }
  },
  "peerDependencies": {
    "react": ">=18 <20",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "dependencies": {
    "@agenteract/core": "workspace:*",
    "@agenteract/dom": "workspace:*"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-flow": "^7.27.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "react-test-renderer": "^19.2.0",
    "ts-jest": "^29.4.5"
  }
}
