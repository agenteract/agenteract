{
  "name": "@agenteract/pty",
  "version": "0.0.3",
  "description": "PTY process manager for Agenteract",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "agenterpty": "./dist/cli.js",
    "@agenteract/pty": "./dist/cli.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "!dist/**/*.tsbuildinfo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/agenteract/agenteract.git",
    "directory": "packages/pty"
  },
  "homepage": "https://github.com/agenteract/agenteract#readme",
  "bugs": {
    "url": "https://github.com/agenteract/agenteract/issues"
  },
  "keywords": [
    "agenteract",
    "agent",
    "pty",
    "terminal",
    "process",
    "coding-agent"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "shx rm -rf dist .tsbuildinfo*"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@agenteract/core": "workspace:*",
    "express": "^4.19.2",
    "node-pty": "^1.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/yargs": "^17.0.33",
    "typescript": "^5.6.0"
  }
}
