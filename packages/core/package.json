{
  "name": "@agenteract/core",
  "version": "0.0.2",
  "description": "Core message schema, bridge protocol, and shared utilities for Agenteract",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/cjs/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/agenteract/agenteract.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/agenteract/agenteract#readme",
  "bugs": {
    "url": "https://github.com/agenteract/agenteract/issues"
  },
  "keywords": [
    "agenteract",
    "agent",
    "bridge",
    "protocol",
    "coding-agent"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:post",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:post": "echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json && echo '{\"type\":\"module\"}' > dist/esm/package.json"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js",
      "types": "./dist/cjs/src/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "typescript": "^5.6.0"
  }
}
